<?xml version="1.0" encoding="utf-8"?>

<node name="/Media_Player_Iface"
      xmlns:tp="http://telepathy.freedesktop.org/wiki/DbusSpec#extensions-v0"
      xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">

	  <interface name="com.meego.UMMS.MediaPlayer">

            <method name="SetUri" tp:name-for-bindings="Set_Uri">
              <arg name="uri" type="s"/>
            </method>

            <method name="Play" tp:name-for-bindings="Play">
 			</method>

            <method name="Pause" tp:name-for-bindings="Pause">
			</method>

            <method name="Stop" tp:name-for-bindings="Stop">
			</method>

		    <method name="SetPosition" tp:name-for-bindings="Set_Position">
              <arg name="pos" type="x"/>
		    </method>

		    <method name="GetPosition" tp:name-for-bindings="Get_Position">
              <arg name="pos" type="x" direction="out"/>
		    </method>

            <method name="SetPlaybackRate" tp:name-for-bindings="Set_Playback_Rate">
              <arg name="rate" type="d"/>
		    </method>

            <method name="GetPlaybackRate" tp:name-for-bindings="Get_Playback_Rate">
              <arg name="rate" type="d" direction="out"/>
		    </method>

		    <method name="SetVolume" tp:name-for-bindings="Set_Volume">
              <arg name="volume" type="i"/>
		    </method>

            <method name="GetVolume" tp:name-for-bindings="Get_Volume">
              <arg name="volume" type="i" direction="out"/>
		    </method>

		    <method name="SetWindowId" tp:name-for-bindings="Set_Window_Id">
              <arg name="win_id" type="d"/>
            </method>

		    <method name="SetVideoSize" tp:name-for-bindings="Set_Video_Size">
              <arg name="x" type="u"/>
              <arg name="y" type="u"/>
              <arg name="w" type="u"/>
              <arg name="h" type="u"/>
		    </method>

            <method name="GetVideoSize" tp:name-for-bindings="Get_Video_Size">
              <arg name="w" type="u" direction="out"/>
              <arg name="h" type="u" direction="out"/>
            </method>

            <method name="GetBufferedTime" tp:name-for-bindings="Get_Buffered_Time">
              <arg type="x" direction="out"/>
		    </method>

            <method name="GetBufferedBytes" tp:name-for-bindings="Get_Buffered_Bytes">
              <arg type="x" direction="out"/>
		    </method>

            <method name="GetMediaSizeTime" tp:name-for-bindings="Get_Media_Size_Time">
              <arg type="x" direction="out"/>
		    </method>

		    <method name="GetMediaSizeBytes" tp:name-for-bindings="Get_Media_Size_Bytes">
              <arg type="x" direction="out"/>
		    </method>

		    <method name="HasVideo" tp:name-for-bindings="Has_Video">
              <arg type="b" direction="out"/>
		    </method>

		    <method name="HasAudio" tp:name-for-bindings="Has_Audio">
              <arg type="b" direction="out"/>
		    </method>

		    <method name="IsStreaming" tp:name-for-bindings="Is_Streaming">
              <arg type="b" direction="out"/>
            </method>
		    
			<method name="IsSeekable" tp:name-for-bindings="Is_Seekable">
              <arg type="b" direction="out"/>
		    </method>

            <method name="SupportFullscreen" tp:name-for-bindings="Support_Fullscreen">
              <arg type="b" direction="out"/>
		    </method>

		    <method name="GetPlayerState" tp:name-for-bindings="Get_Player_State">
              <arg type="i" direction="out"/>
            </method>

            <signal name="Initialized" tp:name-for-bindings="Initialized">
			</signal>

            <signal name="Eof" tp:name-for-bindings="Eof">
			</signal>

            <signal name="Error" tp:name-for-bindings="Error">
              <arg  type="u"/>
              <arg  type="s"/>
		    </signal>

			<signal name="Buffering" tp:name-for-bindings="Buffering">
			</signal>

			<signal name="Buffered" tp:name-for-bindings="Buffered">
			</signal>

			<signal name="RequestWindow" tp:name-for-bindings="Request_Window">
			</signal>

			<signal name="Seeked" tp:name-for-bindings="Seeked">
			</signal>

			<signal name="Stopped" tp:name-for-bindings="Stopped">
			</signal>

			<signal name="PlayerStateChanged" tp:name-for-bindings="Player_State_Changed">
				<arg type="i"/>
			</signal>

          </interface>

</node>
